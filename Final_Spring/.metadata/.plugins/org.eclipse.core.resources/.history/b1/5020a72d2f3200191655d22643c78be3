<script type="text/javascript">
window.onload = function() {
	$('.se2_inputarea').html("${dto.content}");
	$('.se2_inputarea').keyup(function() {
		var content = $('body').html().length;
		top.setCounter(content);	
	});
	$('.se2_inputarea').keyup();
}
</script>
